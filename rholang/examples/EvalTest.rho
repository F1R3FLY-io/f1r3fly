new stdout(`rho:io:stdout`), MatchNilTest, resultCh in {
  contract MatchNilTest(@term, return) = {
    match term {
      "testing" => {
				return!("Handled SomePattern")
      }
      Nil => {
				return!("Handled Nil")
      }
      _ => {
				return!("Handled default case")
      }
    }
  } |

	MatchNilTest!("testing123", *resultCh) |
	for (@result <- resultCh) {
		stdout!(result != Nil)
	}
}
