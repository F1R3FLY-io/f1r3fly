new stdout(`rho:io:stdout`) in {
  // Define a sample list and a function to match on different patterns
  new listCh in {
    listCh!(Nil) |

    for (@list <- listCh) {
      match list {

                Nil => {
                  stdout!("Sucess: Matched an empty list")
                }
                _ => {
                  stdout!("ERORR: Matched a non-empty list")
                }

      }
    }
  }
}
